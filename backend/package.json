{
  "name": "nutribot-backend",
  "version": "2.0.0",
  "description": "Typed backend API for Nutribot Application",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "keywords": [
    "express",
    "postgresql",
    "jwt",
    "auth",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.104.0",
    "pg": "^8.11.3",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.10.7",
    "@types/validator": "^13.11.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.4.2",
    "vitest": "^1.4.0"
  }
}
